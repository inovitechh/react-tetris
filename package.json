{
  "name": "react-tetris",
  "version": "1.0.0",
  "description": "React Tetris oyunu - Android cihazlar için dokunmatik kontrollü",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build:android",
    "export": "expo export --platform web",
    "predeploy": "npm run export",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "expo": "~54.0.10",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "gh-pages": "^6.1.1"
  },
  "private": true
}
